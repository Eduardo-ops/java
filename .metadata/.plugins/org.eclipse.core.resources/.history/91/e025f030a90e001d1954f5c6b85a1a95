package com.domain.projetoyoutube.model;

import com.domain.projetoyoutube.controller.AcoesVideo;

public class Video implements AcoesVideo{

	private String titulo;
	private int avaliacao;
	private int visualizacoes;
	private int curtidas;
	private String reproduzindo;

	public Video(String titulo, int avaliacao, int visualizacoes, int curtidas, String reproduzindo) {
		this.titulo = titulo;
		this.avaliacao = avaliacao;
		this.visualizacoes = visualizacoes;
		this.curtidas = curtidas;
		this.reproduzindo = reproduzindo;
	}

	public Video() {

	}
	
	@Override
	public void play() {
		
	}

	public String getTitulo() {
		return titulo;
	}

	public void setTitulo(String titulo) {
		this.titulo = titulo;
	}

	public int getAvaliacao() {
		return avaliacao;
	}

	public void setAvaliacao(int avaliacao) {
		this.avaliacao = avaliacao;
	}

	public int getVisualizacoes() {
		return visualizacoes;
	}

	public void setVisualizacoes(int visualizacoes) {
		this.visualizacoes = visualizacoes;
	}

	public int getCurtidas() {
		return curtidas;
	}

	public void setCurtidas(int curtidas) {
		this.curtidas = curtidas;
	}

	public String getReproduzindo() {
		return reproduzindo;
	}

	public void setReproduzindo(String reproduzindo) {
		this.reproduzindo = reproduzindo;
	}

}
